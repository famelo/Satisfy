{
    "name": "famelo/distribution-satisfy",
    "description" : "Customer Satisfaction Management",
    "license": "GPL-3.0+",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mneuhaus/TYPO3.Expose.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/afoeder/TYPO3.Asset.git"
        },
        {
            "type": "composer",
            "url": "http://ci.typo3.robertlemke.net/job/composer-packages/ws/repository/"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mneuhaus/Famelo.Satisfy.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mneuhaus/Cobus.Satisfy.git"
        }
    ],
    "config": {
        "vendor-dir": "Packages/Libraries",
        "bin-dir": "bin",
        "preferred-install": "dist"
    },
    "require": {
        "typo3/flow": "*",
        "typo3/expose": "*",
        "typo3/kickstart": "*",
        "typo3/swiftmailer": "*",
        "famelo/satisfy": "*",
        "community/cacheextensions": "*",
        "typo3/setup": "*",
        "fzaninotto/faker": "*",
        "famelo/navigation": "*",
        "famelo/scheduler": "*",
        "gChartPhp/gChartPhp": "*",
        "sandstorm/plumber": "*",
        "famelo/features": "*"
    },
    "require-dev": {
        "typo3/buildessentials": "dev-master",
        "mikey179/vfsstream": "1.1.*",
        "typo3/surf": "dev-master",
        "famelo/surf-sharedhosting": "dev-master"
    },
    "minimum-stability": "dev",
    "scripts": {
        "post-update-cmd": "TYPO3\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-install-cmd": "TYPO3\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall"
    }
}
